{
    "Person": {
        "FirstName": {
            "rules": {
                "required": true,
                "maxlength": 15
            }
        },
        "LastName": {
            "rules": {
                "required": true,
                "maxlength": 15
            }
        },
        "Contact": {
            "Email": {
                "rules": {
                    "required": true,
                    "maxlength": 100,
                    "email": true
                }
            }
        }
    },
    "Hobbies": [
        {
            "HobbyName": {
                "rules": {
                    "required": true,
                    "maxlength": 100
                }
            },
            "Frequency": {
                "rules": {
                    "enum": ["Daily", "Weekly", "Monthly"]
                }
            }
        },
        {
            "maxItems": 4,
            "minItems": 2
        }
    ]
}